  schema:rangeIncludes okh:WebsiteUrl ;
  owl:cardinality 1 ;
  schema:domainIncludes okh:Module ;
  okhmeta:hasMetaTag okhmeta:HistoryTag ;
  .

okh:version
  a owl:DatatypeProperty ;
  a skos:Concept ;
  rdfs:label "version"@en ;
