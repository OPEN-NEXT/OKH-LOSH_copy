  rdfs:subPropertyOf schema:inLanguage ;
  vs:term_status "unstable" ;
  schema:domainIncludes
    okh:Module,
    okh:Part ;
  .

okh:licensor
  a owl:ObjectProperty ;
  a skos:Concept ;
  rdfs:subPropertyOf dcterms:rightsHolder ;
