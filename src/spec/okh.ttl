okh:SourceFile
  a owl:Class ;
  a skos:Concept ;
  rdfs:label "source file"@en ;
  rdfs:subClassOf okh:File ;
  rdfs:subClassOf schema:TextDigitalDocument ;
  vs:term_status "unstable" ;
  okhmeta:hasMetaTag okhmeta:DataTag ;
  .

okh:ExportFile
  a owl:Class ;
  a skos:Concept ;
  rdfs:label "exported file"@en ;
  rdfs:subClassOf okh:File ;
  vs:term_status "unstable" ;
