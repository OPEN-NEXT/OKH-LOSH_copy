  rdfs:subClassOf dcterms:MethodOfInstruction ;
  rdfs:subClassOf schema:TextDigitalDocument ;
  vs:term_status "unstable" ;
  okhmeta:hasMetaTag okhmeta:DocmentationTag ;
  .

okh:ContributionGuide
  a owl:Class ;
  a skos:Concept ;
  rdfs:label "contribution guide"@en ;
  rdfs:subClassOf okh:File ;
  rdfs:subClassOf dcterms:MethodOfInstruction ;
  rdfs:subClassOf schema:TextDigitalDocument ;
  vs:term_status "unstable" ;
  okhmeta:hasMetaTag okhmeta:DocmentationTag ;
  okhmeta:hasMetaTag okhmeta:ExtendedInfoTag ;
