okh:UserManual
  a owl:Class ;
  a skos:Concept ;
  rdfs:label "User Manual"@en ;
  rdfs:subClassOf okh:File ;
  rdfs:subClassOf dcterms:MethodOfInstruction ;
  vs:term_status "unstable" ;
  okhmeta:hasMetaTag okhmeta:DocumentationTag ;
