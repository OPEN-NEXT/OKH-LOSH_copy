#  rdfs:subPropertyOf spdxt:License ;
  vs:term_status "unstable" ;
  schema:domainIncludes okh:Module ;
  schema:rangeIncludes okh:File ;
  owl:cardinality 1 ;
  okhmeta:hasMetaTag okhmeta:LegalTag ;
  .

