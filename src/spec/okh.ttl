      foaf:name "Jane Doe" ;
      foaf:nick "jd" ;
      foaf:topic_interest "Open Source Hardware" ;
      foaf:topic_interest "Art" ;
    ] ;
  okhmeta:hasMetaTag okhmeta:LegalTag ;
  .

okh:organization
  a owl:ObjectProperty ;
  a skos:Concept ;
