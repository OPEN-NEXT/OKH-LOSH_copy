  rdfs:subPropertyOf schema:url ;
  vs:term_status "unstable" ;
  schema:domainIncludes okh:Module ;
  owl:cardinality 1 ;
  okhmeta:hasMetaTag okhmeta:DataTag ;
  .

okh:repoHost
